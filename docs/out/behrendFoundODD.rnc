    teimacro.phraseSeq,
    teiatt.global.attributes,
    teiatt.typed.attribute.subtype,
    teiatt.measurement.attributes,
    teiatt.ranging.attributes,
    
    ## specifies the type of measurement in any convenient typology.
    attribute type {
